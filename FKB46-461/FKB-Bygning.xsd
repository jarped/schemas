  <simpleType name="BygningsstatusKodeType">
    <annotation>
      <documentation>Lovlige statuser for bygningstiltak.</documentation>
      <appinfo>
        <taggedValue xmlns="http://www.interactive-instruments.de/ShapeChange/AppInfo" tag="SOSI_navn">BYGGSTAT</taggedValue>
