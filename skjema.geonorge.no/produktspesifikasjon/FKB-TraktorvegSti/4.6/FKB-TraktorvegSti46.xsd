            </annotation>
          </element>
          <element name="vegsperringtype" type="app:VegsperringtypeType">
            <annotation>
              <documentation>angir type vegsperring</documentation>
              <appinfo>
                <taggedValue xmlns="http://www.interactive-instruments.de/ShapeChange/AppInfo" tag="SOSI_navn">VEGSPERRINGTYPE</taggedValue>
              </appinfo>
            </annotation>
          </element>
          <element name="kommunenummer" type="gml:CodeType">
            <annotation>
              <documentation>nummerering av kommuner i henhold til SSB sin offisielle liste</documentation>
              <appinfo>
                <defaultCodeSpace xmlns="http://www.opengis.net/gml/3.2">http://skjema.geonorge.no/SOSI/produktspesifikasjon/FKB-TraktorvegSti/4.6/Kommunenummer.xml</defaultCodeSpace>
